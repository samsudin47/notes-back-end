{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "quotes": ["error", "double"],
    "eol-last": ["error", "never"],
    "no-unused-vars": "off",
    "indent": "off",
    "linebreak-style": 0,
    "comma-dangle": ["never"]
  }
}
